<h1>Listing author_photos</h1>

<table>
  <tr>
    <th>Author</th>
    <th>Photo</th>
  </tr>

<% for author_photo in @author_photos %>
  <tr>
    <td><%=h author_photo.author_id %></td>
    <td><%=h author_photo.photo_id %></td>
    <td><%= link_to 'Show', author_photo %></td>
    <td><%= link_to 'Edit', edit_author_photo_path(author_photo) %></td>
    <td><%= link_to 'Destroy', author_photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New author_photo', new_author_photo_path %>
