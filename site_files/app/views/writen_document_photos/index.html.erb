<h1>Listing writen_document_photos</h1>

<table>
  <tr>
    <th>Writen document</th>
    <th>Photo</th>
  </tr>

<% for writen_document_photo in @writen_document_photos %>
  <tr>
    <td><%=h writen_document_photo.writen_document_id %></td>
    <td><%=h writen_document_photo.photo_id %></td>
    <td><%= link_to 'Show', writen_document_photo %></td>
    <td><%= link_to 'Edit', edit_writen_document_photo_path(writen_document_photo) %></td>
    <td><%= link_to 'Destroy', writen_document_photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

