<h1>Listing sound_document_photos</h1>

<table>
  <tr>
    <th>Sound document</th>
    <th>Photo</th>
  </tr>

<% for sound_document_photo in @sound_document_photos %>
  <tr>
    <td><%=h sound_document_photo.sound_document_id %></td>
    <td><%=h sound_document_photo.photo_id %></td>
    <td><%= link_to 'Show', sound_document_photo %></td>
    <td><%= link_to 'Edit', edit_sound_document_photo_path(sound_document_photo) %></td>
    <td><%= link_to 'Destroy', sound_document_photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New sound_document_photo', new_sound_document_photo_path %>
