<h1>Editing photo_locals</h1>

<% form_for(@photo_locals) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :photo_id %><br />
    <%= f.text_field :photo_id %>
  </p>
  <p>
    <%= f.label :local_id %><br />
    <%= f.text_field :local_id %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @photo_locals %> |
<%= link_to 'Back', photo_locals_path %>
