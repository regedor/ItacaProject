<h1>Editing prize_locals</h1>

<% form_for(@prize_locals) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :prize_id %><br />
    <%= f.text_field :prize_id %>
  </p>
  <p>
    <%= f.label :local_id %><br />
    <%= f.text_field :local_id %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @prize_locals %> |
<%= link_to 'Back', prize_locals_path %>
