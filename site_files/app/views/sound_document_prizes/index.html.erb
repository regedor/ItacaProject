<h1>Listing sound_document_prizes</h1>

<table>
  <tr>
    <th>Sound document</th>
    <th>Prize</th>
    <th>Description</th>
  </tr>

<% for sound_document_prize in @sound_document_prizes %>
  <tr>
    <td><%=h sound_document_prize.sound_document_id %></td>
    <td><%=h sound_document_prize.prize_id %></td>
    <td><%=h sound_document_prize.description %></td>
    <td><%= link_to 'Show', sound_document_prize %></td>
    <td><%= link_to 'Edit', edit_sound_document_prize_path(sound_document_prize) %></td>
    <td><%= link_to 'Destroy', sound_document_prize, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New sound_document_prize', new_sound_document_prize_path %>
