<%= f.error_messages %>
<%= f.persist_fields %>

<dl>
  <%= field_selector f, "author"                            %>
  <% f.columns.each do |column| -%>
    <%= f.render column unless ['created_at','updated_at'].include? column.name %>
  <% end -%>
  <%= field_selector f, "category"                          %>
  <%= field_selector f, "subcategory_1_id", Subcategory     %>
  <%= field_selector f, "subcategory_2_id", Subcategory     %>
  <%= field_selector f, "subcategory_3_id", Subcategory     %>
  <%= field_selector f, "subcategory_4_id", Subcategory     %>
  <br/>
</dl>
 
