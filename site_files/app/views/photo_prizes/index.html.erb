<h1>Listing photo_prizes</h1>

<table>
  <tr>
    <th>Photo</th>
    <th>Prize</th>
    <th>Description</th>
  </tr>

<% for photo_prize in @photo_prizes %>
  <tr>
    <td><%=h photo_prize.photo_id %></td>
    <td><%=h photo_prize.prize_id %></td>
    <td><%=h photo_prize.description %></td>
    <td><%= link_to 'Show', photo_prize %></td>
    <td><%= link_to 'Edit', edit_photo_prize_path(photo_prize) %></td>
    <td><%= link_to 'Destroy', photo_prize, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New photo_prize', new_photo_prize_path %>
