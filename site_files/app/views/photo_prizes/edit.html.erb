<h1>Editing photo_prize</h1>

<% form_for(@photo_prize) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :photo_id %><br />
    <%= f.text_field :photo_id %>
  </p>
  <p>
    <%= f.label :prize_id %><br />
    <%= f.text_field :prize_id %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @photo_prize %> |
<%= link_to 'Back', photo_prizes_path %>
