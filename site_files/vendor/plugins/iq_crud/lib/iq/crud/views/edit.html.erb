<h1>Editar <%= resource_singular.humanize.downcase %></h1>

<% form_for current_member, :html => { :multipart => true } do |f| %>
<%= render :partial => 'form', :locals => { :f => f } %>

<p class="form-actions">
  <%= submit_tag "Guardar alterações"%> <span>or</span>
  <%= link_to 'cancelar', collection_path, :title => "Voltar para lista de #{resource_plural.humanize.downcase}" %>
</p>

<% end %>
